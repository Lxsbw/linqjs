{
  "name": "linq-to-javascript",
  "version": "2.0.2",
  "description": "Linq + JavaScript",
  "main": "src/linq.js",
  "types": "src/linq.d.ts",
  "files": [
    "src/*.js",
    "src/*.d.ts"
  ],
  "scripts": {
    "build": "uglifyjs ./src/linq.js --output ./build/linq.min.js --compress --mangle",
    "buildes": "uglifyjs ./src/es5/linq.js --output ./build/linq.es5.min.js --compress --mangle",
    "cbuild": "coffee -o ./build/clinq.js -c -b ./src/coffee/linq.coffee",
    "test": "jest",
    "testc": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lxsbw/linqjs.git"
  },
  "keywords": [
    "linq",
    "javascript"
  ],
  "author": {
    "name": "lxsbw",
    "email": "lxsbw@outlook.com",
    "url": "https://github.com/Lxsbw"
  },
  "license": "MIT",
  "homepage": "https://github.com/Lxsbw/linqjs#README",
  "dependencies": {},
  "devDependencies": {
    "jest": "~29.7.0"
  }
}
